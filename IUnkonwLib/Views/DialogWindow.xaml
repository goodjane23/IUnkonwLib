<Window x:Class="IUnkonwLib.Views.DialogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        WindowStyle="None"
        Title="DialogWindow"
        WindowStartupLocation="CenterScreen"
        SizeToContent="WidthAndHeight">
    <Grid HorizontalAlignment="Center" VerticalAlignment="Center">
        <StackPanel Orientation="Vertical">
            <TextBlock x:Name="MessageTbk" FontSize="20" Margin="10" TextWrapping="Wrap"/>
            <StackPanel Orientation="Horizontal">
                <Button Content="Продолжить" Margin="10" Click="Button_Click"/>
                <Button Content="Выйти" Margin="10" Click="Button_Click_1"/>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>